  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Javey/intact-vue/issues"
  },
  "homepage": "https://github.com/Javey/intact-vue#readme",
  "peerDependencies": {
    "vue": "^3.0.0",
    "intact": "^2.5.4"
  },
